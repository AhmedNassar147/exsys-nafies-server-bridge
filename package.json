{
  "name": "exsys-nafies-server-bridge",
  "version": "1.0.0",
  "private": true,
  "author": "ahmed nassar",
  "license": "ISC",
  "sideEffects": false,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rm -rf ./node_modules",
    "clean-locks": "rm -rf ./yarn.lock",
    "bootstrap": "lerna bootstrap",
    "reset": "yarn clean && yarn clean-locks && yarn bootstrap",
    "prepare": "yarn workspace @exsys-server/start-exsys-nafies-bridge start",
    "start": "yarn workspace @exsys-server/bridge start"
  },
  "dependencies": {
    "@exsys-server/bridge": "1.0.0",
    "@exsys-server/helpers": "1.0.0",
    "@exsys-server/start-exsys-nafies-bridge": "1.0.0",
    "axios": "^0.26.1",
    "chalk": "^5.0.1",
    "https": "^1.0.0",
    "is-online": "^9.0.1",
    "inquirer": "^8.2.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
